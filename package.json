{
  "name": "frontend-observability-demo",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "clean": "turbo run clean",
    "dev": "turbo dev --filter=web --filter=product-api --filter=recommend-api",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "test": "turbo run test"
  },
  "devDependencies": {
    "prettier": "^3.2.5",
    "turbo": "^2.0.5",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "yarn@1.22.19",
  "workspaces": [
    "apps/*",
    "packages/*"
  ]
}
